        </swrc_ext:authorList>
        <rdfs:label>Benchmark Suites for Improving the RDF(S) Importers and Exporters of Ontology Development Tools</rdfs:label>
        <owl:sameAs rdf:resource="http://www.eswc2006.org/full-papers/#garciacastro-gomezperez"/>

            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/malte-kiesel"/>
        </swrc_ext:authorList>
        <rdfs:label>RDFBroker: A Signature-Based High-Performance RDF Store</rdfs:label>
        <owl:sameAs rdf:resource="http://www.eswc2006.org/full-papers/#sintek-kiesel"/>

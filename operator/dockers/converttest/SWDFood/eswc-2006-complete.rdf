            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/javier-esplugas-cuadrado"/>
        </swrc_ext:authorList>
        <rdfs:label>Protocol Mediation for Adaptation in Semantic Web Services</rdfs:label>
        <owl:sameAs rdf:resource="http://www.eswc2006.org/full-papers/#williams-battle"/>

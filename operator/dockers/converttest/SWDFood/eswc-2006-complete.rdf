        </swrc_ext:authorList>
        <rdfs:label>Interactive Ontology-Based User Knowledge Acquisition: A Case Study</rdfs:label>
        <owl:sameAs rdf:resource="http://www.eswc2006.org/full-papers/#aroyo-denaux"/>
        <maker rdf:resource="http://data.semanticweb.org/person/lora-aroyo"/>

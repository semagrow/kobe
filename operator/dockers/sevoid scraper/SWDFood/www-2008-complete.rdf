    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2008/paper/606">
        <swc:hasRelatedEvent rdf:resource="http://data.semanticweb.org/conference/www/2008/track/semantic-web/talk/606"/>
        <swc:hasTopic rdf:resource="http://dbpedia.org/resource/World_Wide_Web"/>
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2008/proceedings"/>

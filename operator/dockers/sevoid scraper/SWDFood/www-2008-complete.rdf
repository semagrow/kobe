    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2008/paper/138">
        <swc:hasRelatedEvent rdf:resource="http://data.semanticweb.org/conference/www/2008/track/internet-monetization/talk/138"/>

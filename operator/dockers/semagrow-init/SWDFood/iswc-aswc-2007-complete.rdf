        <swrc:title>Correspondence Patterns for Ontology Mediation</swrc:title>
        <swrc:url rdf:resource="http://iswc2007.semanticweb.org/papers/Paper339-correspondence-patterns.pdf"/>
        <swrc:year>2007</swrc:year>

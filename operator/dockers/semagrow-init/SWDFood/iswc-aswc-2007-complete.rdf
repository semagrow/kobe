        </swrc_ext:authorList>
        <owl:sameAs rdf:resource="http://revyu.com/things/iswc-aswc-2007-doctoral-consortium-paper-920-ontologydriven-management"/>
        <maker rdf:resource="http://data.semanticweb.org/person/reto-krummenacher"/>

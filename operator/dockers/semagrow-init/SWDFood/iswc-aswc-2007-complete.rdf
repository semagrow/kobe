        <swrc:author rdf:resource="http://data.semanticweb.org/person/fabian-abel"/>
        <swrc:author rdf:resource="http://data.semanticweb.org/person/juri-de-coi"/>
        <swrc:author rdf:resource="http://data.semanticweb.org/person/nicola-henze"/>
        <swrc:month>November</swrc:month>
        <swrc:pages>1--14</swrc:pages>
        <swrc:title>Enabling Advanced and Context-Dependent Access Control in RDF Stores</swrc:title>
        <swrc:url rdf:resource="http://data.semanticweb.org/pdfs/iswc-aswc/2007/ISWC2007_RT_Abel.pdf"/>

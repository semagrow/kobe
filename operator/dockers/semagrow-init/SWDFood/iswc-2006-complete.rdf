            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/tim-finin"/>
        </swrc_ext:authorList>
        <rdfs:label>Characterizing the Semantic Web on the Web</rdfs:label>
        <maker rdf:resource="http://data.semanticweb.org/person/li-ding"/>
        <maker rdf:resource="http://data.semanticweb.org/person/tim-finin"/>

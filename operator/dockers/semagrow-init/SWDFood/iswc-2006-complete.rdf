            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/simon-harper"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/darren-lunn"/>
        </swrc_ext:authorList>
        <rdfs:label>SADIe: Semantic Annotation for Accessibility</rdfs:label>

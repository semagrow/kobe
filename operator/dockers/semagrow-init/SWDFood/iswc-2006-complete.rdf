            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/bijan-parsia"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/jim-hendler"/>
        </swrc_ext:authorList>
        <rdfs:label>A Survey of the Web Ontology Landscape</rdfs:label>
        <maker rdf:resource="http://data.semanticweb.org/person/bijan-parsia"/>

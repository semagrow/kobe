            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/christopher-brewster"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/nigel-r-shadbolt"/>
        </swrc_ext:authorList>
        <rdfs:label>Ranking Ontologies with AKTiveRank</rdfs:label>
        <maker rdf:resource="http://data.semanticweb.org/person/christopher-brewster"/>

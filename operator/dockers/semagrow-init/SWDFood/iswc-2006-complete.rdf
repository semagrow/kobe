            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/marta-sabou"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/enrico-motta"/>
        </swrc_ext:authorList>
        <rdfs:label>PowerMap: Mapping the Real Semantic Web on the Fly</rdfs:label>
        <maker rdf:resource="http://data.semanticweb.org/person/enrico-motta"/>

            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/alexandra-poulovassilis"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/peter-wood"/>
        </swrc_ext:authorList>
        <rdfs:label>A Relaxed Approach to RDF Querying</rdfs:label>

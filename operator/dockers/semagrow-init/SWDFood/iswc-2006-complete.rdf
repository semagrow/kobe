            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/emilia-cimpian"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/mick-kerrigan"/>
        </swrc_ext:authorList>
        <rdfs:label>Formal Model for Ontology Mapping Creation</rdfs:label>

            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/jeff-z-pan"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/uwe-thaden"/>
        </swrc_ext:authorList>
        <rdfs:label>Querying the Semantic Web with Preferences</rdfs:label>

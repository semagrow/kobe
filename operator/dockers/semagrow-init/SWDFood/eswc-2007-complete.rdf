        <swrc:keywords>Benchmark</swrc:keywords>
        <swrc:keywords>Evaluation</swrc:keywords>
        <swrc:keywords>OWL</swrc:keywords>
        <swrc:keywords>Reasoning</swrc:keywords>
        <swrc:url rdf:resource="http://www.eswc2007.org/pdf/eswc07-weithoener.pdf"/>
        <rdfs:label>Real-world Reasoning with OWL</rdfs:label>

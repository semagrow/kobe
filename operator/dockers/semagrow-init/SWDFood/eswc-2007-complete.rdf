        <swrc:keywords>Natural language interface</swrc:keywords>
        <swrc:keywords>Ontology</swrc:keywords>
        <swrc:keywords>SPARQL</swrc:keywords>
        <swrc:url rdf:resource="http://www.eswc2007.org/pdf/eswc07-wang.pdf"/>
        <rdfs:label>PANTO: A Portable Natural Language Interface to Ontologies</rdfs:label>

        <swrc:author rdf:resource="http://data.semanticweb.org/person/azzurra-ragone"/>
        <swrc:keywords>DLR-Lite</swrc:keywords>
        <swrc:keywords>Fuzzy Logic</swrc:keywords>
        <swrc:keywords>P2P marketplace</swrc:keywords>
        <swrc:keywords>Semantic matchmaking</swrc:keywords>
        <swrc:url rdf:resource="http://www.eswc2007.org/pdf/eswc07-ragone.pdf"/>

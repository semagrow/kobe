	<!-- added a lot of proper affiliation links with a simple SPARQL construct query to the dogfood server
	PREFIX swrc: <http://swrc.ontoware.org/ontology#>
	PREFIX foaf: <http://xmlns.com/foaf/0.1/>
	PREFIX swc:  <http://data.semanticweb.org/ns/swc/ontology#>
	CONSTRUCT {
		?person swrc:affiliation ?affiliation .
	} WHERE {
		?affiliation foaf:name ?affiliation_name .
		GRAPH <http://data.semanticweb.org/conference/eswc/2007> {
			?person swc:affiliation ?affiliation_name .
		}
	}
-->


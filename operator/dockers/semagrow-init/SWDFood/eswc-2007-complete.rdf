            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/kim-viljanen"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/eero-hyvoenen"/>
        </swrc_ext:authorList>
    </swc:Paper>

    <swc:SystemDescription rdf:about="http://data.semanticweb.org/conference/eswc/2007/paper-202">
        <swc:uuid>f0ee429d-b300-4a67-92b0-5bca33f3b251</swc:uuid>
        <dc:title>Caravela: Semantic Content Management with Automatic Content Integration and Categorization</dc:title>
        <swrc:abstract>Semantic web content management poses much manual work onto the community. To reduce this labour we have devised a generic approach to dynamic content integration and automatic categorization. Content and documents of different types can be integrated from diverse semi-structured sources and categorized along multiple dimensions. Automatic linking provides dynamic categorizations at no user cost. We illustrate our approach by an online bibliography categorizing scientific research publications.</swrc:abstract>
        <swrc:author rdf:resource="http://data.semanticweb.org/person/erhard-rahm"/>
        <swrc:author rdf:resource="http://data.semanticweb.org/person/david-aumueller"/>
        <swrc:keywords>automatisms</swrc:keywords>
        <swrc:keywords>categorization</swrc:keywords>
        <swrc:keywords>dynamic linking</swrc:keywords>
        <swrc:keywords>information integration</swrc:keywords>
        <swrc:keywords>semantic content management</swrc:keywords>
        <swrc:url rdf:resource="http://www.eswc2007.org/pdf/eswc07-aumueller.pdf"/>
        <rdfs:label>Caravela: Semantic Content Management with Automatic Content Integration and Categorization</rdfs:label>
        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>
        <swrc_ext:authorList rdf:parseType="Resource">
            <rdf:_1 rdf:resource="http://data.semanticweb.org/person/david-aumueller"/>
            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/erhard-rahm"/>
        </swrc_ext:authorList>

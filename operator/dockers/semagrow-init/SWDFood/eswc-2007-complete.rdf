        <swrc:keywords>web mining</swrc:keywords>
        <swrc:url rdf:resource="http://www.eswc2007.org/pdf/eswc07-jin.pdf"/>
        <rdfs:label>Extracting Social Networks among Various Entities on the Web</rdfs:label>
        <swrc_ext:authorList rdf:parseType="Resource">

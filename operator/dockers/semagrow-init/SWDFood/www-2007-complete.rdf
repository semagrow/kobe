    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2007/paper/main/216">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2007/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/krithi-ramamritham"/>

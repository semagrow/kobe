            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/ludger-martin"/>
        </swrc_ext:authorList>
        <rdfs:label>Estimating Web Site Readability Using Content Extraction</rdfs:label>
    </swrc:InProceedings>

    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2009/paper/166">

            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/fidel-cacheda"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/victor-carneiro"/>
            <rdf:_4 rdf:resource="http://data.semanticweb.org/person/vreixo-formoso"/>
            <rdf:_5 rdf:resource="http://data.semanticweb.org/person/raffaele-perego"/>
            <rdf:_6 rdf:resource="http://data.semanticweb.org/person/fabrizio-silvestri"/>
        </swrc_ext:authorList>
        <rdfs:label>Search Shortcuts: Driving Users Towards Their Goals</rdfs:label>

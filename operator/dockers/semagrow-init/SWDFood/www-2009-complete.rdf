            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/juanzi-li"/>
            <rdf:_4 rdf:resource="http://data.semanticweb.org/person/lizhu-zhou"/>
        </swrc_ext:authorList>
        <rdfs:label>Discovering the Staring People From Social Networks</rdfs:label>
    </swrc:InProceedings>

    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2009/paper/191">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2009/proceedings"/>
        <dc:subject>Poster Session</dc:subject>
        <dct:title>Analysis of Community Structure in Wikipedia</dct:title>
        <swrc:abstract>We present the results of a community detection analysis of the Wikipedia graph. Distinct communities in Wikipedia contain semantically closely related articles. The central topic of a community can be identified using PageRank. Extracted communities can be organized hierarchically similar to manually created Wikipedia category structure.</swrc:abstract>
        <swrc:month>April</swrc:month>
        <swrc:url rdf:resource="http://www2009.eprints.org/191/1/p1221.pdf"/>
        <swrc:year>2009</swrc:year>
        <swrc_ext:authorList rdf:parseType="Resource">
            <rdf:_1 rdf:resource="http://data.semanticweb.org/person/dmitry-lizorkin"/>
            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/olena-medelyan"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/maria-grineva"/>
        </swrc_ext:authorList>
        <rdfs:label>Analysis of Community Structure in Wikipedia</rdfs:label>

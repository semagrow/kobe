            <rdf:_1 rdf:resource="http://data.semanticweb.org/person/munmun-de-choudhury"/>
            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/hari-sundaram"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/ajita-john"/>
            <rdf:_4 rdf:resource="http://data.semanticweb.org/person/doree-duncan-seligmann"/>
        </swrc_ext:authorList>
        <rdfs:label>What Makes Conversations Interesting? Themes, Participants and Consequences of Conversations in Online Social Media</rdfs:label>

            <rdf:_5 rdf:resource="http://data.semanticweb.org/person/mingcheng-qu"/>
            <rdf:_6 rdf:resource="http://data.semanticweb.org/person/jing-shen"/>
            <rdf:_7 rdf:resource="http://data.semanticweb.org/person/jiajun-bu"/>
            <rdf:_8 rdf:resource="http://data.semanticweb.org/person/chun-chen"/>
        </swrc_ext:authorList>
        <rdfs:label>Advertising Keyword Generation Using Active Learning</rdfs:label>

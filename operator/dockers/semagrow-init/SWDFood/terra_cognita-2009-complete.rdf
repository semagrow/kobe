    <swrc:InProceedings rdf:about="http://data.semanticweb.org/workshop/terra_cognita/2009/paper/main/6">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/workshop/terra_cognita/2009/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/arne-broering"/>

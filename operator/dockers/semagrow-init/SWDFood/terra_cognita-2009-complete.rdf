            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/sven-schade"/>
            <rdf:_3 rdf:resource="http://data.semanticweb.org/person/arne-broering"/>
            <rdf:_4 rdf:resource="http://data.semanticweb.org/person/carsten-kessler"/>
            <rdf:_5 rdf:resource="http://data.semanticweb.org/person/christoph-stasch"/>
